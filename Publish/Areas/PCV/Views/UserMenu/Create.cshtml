@model LILI_CRM.Web.Areas.PCV.ViewModel.UserMenuModel 
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/PCV/Views/Shared/_Layout.cshtml";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@*@using (Ajax.BeginForm("Create", "IndentRequest", new AjaxOptions { HttpMethod = "POST" }, new { id = "frm" }))*@
@using (Html.BeginForm("Create", "UserMenu", FormMethod.Post, new { id = "frm" }))
{
    <fieldset> 
        @Html.Partial("_CreateOrEdit", Model)        
        <div class="button-crude">           
            <input type="submit" value="Save" id="btnSave" name="btnSubmit" style="margin-right: 8px;" />
        </div>
    </fieldset>
}
<div class="button-crude button-left">
    @Html.ActionLink("Back to List", "Index")
</div>

