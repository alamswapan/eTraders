@{
    ViewBag.Title = "VoucherInfo";
    Layout = "~/Areas/PCV/Views/Shared/_Layout.cshtml";
}
<div id="breadcrumbs">
    <ul class="breadcrumb">
        <li><i class="icon-home"></i>Setup <span class="divider"><i class="icon-angle-right"></i></span></li>
        <li class="active"><a href="#">VoucherInfo</a></li>
    </ul>
    <!--.breadcrumb-->
</div>
<div id="page-content" class="clearfix">
    <!--/#page-content-->
    <div class="row-fluid">
        <div id="indexPageVoucherInfo">
            <div id="voucherInfoMessageZone">
                <div id="message" class="alert alert-success" style="display: none;"></div>
            </div>
            <div id="voucherInfoAddButtonZone" class="row-fluid">
                @*@if ((new AIMS_UNDP.Web.Utility.AppConstant()).IsAddAssigned)
                {*@
                    <a class="btn btn-mini btn-primary" id="lnkAddVoucherInfo" href='@Url.Content("~/PCV/PettyCashVoucher/Create")'>
                        <i class="icon-plus"></i>Add
                    </a>
                @*}*@
            </div>
            <br />
            <div id="voucherInfoGridZone" class="row-fluid">
                <div id="voucherInfoKendoGrid"></div>
            </div>
            <div id="voucherInfoPopUpZone">
                <div id="addVoucherInfoDialog" title="Add Voucher Info"></div>
                <div id="editVoucherInfoDialog" title="Edit Voucher Info"></div>
                <div id="deleteVoucherInfoDialog" title="Delete Voucher Info"></div>
                <div id="detailsVoucherInfoDailog" title="View Voucher Info"></div>
            </div>
        </div>
        <script src="@Url.Content("~/Scripts/App/voucherInfo-jq.js")" type="text/javascript"></script>
    </div>
</div>

<script type="text/javascript">

    $(function () {
        $(".ui-selectee").removeClass("active");
        $(".ui-selectee").removeClass("open");

        $(".mnuVoucherInfo").addClass("active");
        $(".mnuVoucherInfo").parent().parent().addClass("active").addClass("open");
        $(".mnuVoucherInfo").parent().parent().addClass("open");
    });



</script>